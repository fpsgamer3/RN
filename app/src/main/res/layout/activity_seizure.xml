<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Seizure">

    <TextView
            android:text="Is the person on the ground?
-> If yes, cushion their head."
            android:textSize="9pt"
            android:layout_width="274dp"
            android:layout_height="50dp" android:id="@+id/textView"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintVertical_bias="0.582" app:layout_constraintHorizontal_bias="0.496"/>
    <Button
            android:text="Next"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button5"
            app:layout_constraintTop_toBottomOf="@+id/textView"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintVertical_bias="0.677" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.498"/>
    <ImageView
            android:layout_width="410dp"
            android:layout_height="399dp"
            app:srcCompat="@drawable/aid9420704_v4_728px_help_someone_who_is_having_a_seizure_step_2_version_4"
            android:id="@+id/imageView3"
            app:layout_constraintBottom_toTopOf="@+id/textView" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.0"/>
    <ImageView
            android:layout_width="288dp"
            android:layout_height="291dp" app:srcCompat="@drawable/_12" android:id="@+id/imageView4"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="232dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="584dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>