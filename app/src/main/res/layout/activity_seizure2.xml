<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Seizure">

    <TextView
            android:text="Is the person having difficulty breathing?"
            android:textSize="7pt"
            android:layout_width="334dp"
            android:layout_height="31dp" android:id="@+id/textView"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintVertical_bias="0.65" app:layout_constraintHorizontal_bias="0.506"/>
    <TextView
            android:text="-> If yes, call an ambulance right away."
            android:textSize="7pt"
            android:layout_width="332dp"
            android:layout_height="83dp" android:id="@+id/textView1"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.493"
            android:layout_marginBottom="112dp"/>
    <Button
            android:text="Next"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button6"
            app:layout_constraintTop_toBottomOf="@+id/textView"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintVertical_bias="0.675" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.498"/>
    <ImageView
            android:layout_width="409dp"
            android:layout_height="395dp" app:srcCompat="@drawable/aid9420704_v4_728px_help_fire_victims_step_8"
            android:id="@+id/imageView2"
            app:layout_constraintBottom_toTopOf="@+id/textView" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
    <ImageView
            android:layout_width="288dp"
            android:layout_height="291dp" app:srcCompat="@drawable/_12" android:id="@+id/imageView4"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="232dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="584dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>